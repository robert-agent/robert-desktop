{
  "name": "extract-title",
  "description": "Navigate to a page and extract its title",
  "created": "2025-10-09T00:00:00Z",
  "author": "Claude",
  "tags": ["example", "data-extraction"],
  "cdp_commands": [
    {
      "method": "Page.navigate",
      "params": {
        "url": "https://example.com"
      },
      "description": "Navigate to example.com"
    },
    {
      "method": "Runtime.evaluate",
      "params": {
        "expression": "document.title",
        "returnByValue": true
      },
      "save_as": "page-title.json",
      "description": "Extract page title"
    },
    {
      "method": "Runtime.evaluate",
      "params": {
        "expression": "document.querySelector('h1').textContent",
        "returnByValue": true
      },
      "save_as": "main-heading.json",
      "description": "Extract main heading"
    }
  ]
}
